#   throw errors if there is an existing list value)
# $(CUSTOM_DATA_LIST=id),$(CUSTOM_DATA_LIST=id+=stringToAppend),$(CUSTOM_DATA_LIST=id+#=numberToAppend),$(CUSTOM_DATA_LIST=id=@=numberIndexToGet),$(CUSTOM_DATA_LIST=id-@=numberIndexToRemove)
