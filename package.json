{
    "name": "moonpiebot",
    "version": "1.0.33-beta.0",
    "description": "A custom Twitch chat bot",
    "type": "module",
    "exports": "./dist/index.mjs",
    "imports": {
        "#package.json": "./package.json"
    },
    "bin": {
        "moonpiebot": "./dist/index.mjs"
    },
    "main": "./dist/index.mjs",
    "pkg": {
        "scripts": [
            "dist/**/*.mjs",
            "node_modules/es-get-iterator/node.js",
            "node_modules/sqlite3/lib/binding/**/*.node"
        ],
        "assets": [],
        "outputPath": "./bin"
    },
    "directories": {
        "test": "test"
    },
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "build:dev": "tsc --sourceMap --removeComments false --pretty",
        "build": "tsc",
        "create": "concurrently \"npm run create:envExamples\" \"npm run create:ccCbExamples\" \"npm run create:manPage\" \"npm run create:pkgbuilds\" \"npm run create:windowsInstallerConfig\" \"npm run create:customStrings\" \"npm run create:updateReadme\"",
        "create:customStrings": "ts-node --project tsconfig.eslint.json --esm scripts/createCustomStrings.mts",
        "create:envExamples": "ts-node --project tsconfig.eslint.json --esm scripts/createEnvExamples.mts",
        "create:ccCbExamples": "ts-node --project tsconfig.eslint.json --esm scripts/createCcCbExamples.mts",
        "create:manPage": "ts-node --project tsconfig.eslint.json --esm scripts/createManPage.mts",
        "create:updateReadme": "ts-node --project tsconfig.eslint.json --esm scripts/updateReadme.mts",
        "create:pkgbuilds": "ts-node --project tsconfig.eslint.json --esm scripts/createPkgbuildFiles.mts",
        "create:windowsInstallerConfig": "ts-node --project tsconfig.eslint.json --esm scripts/createWindowsInstallerConfigFiles.mts",
        "dev": "cross-env NODE_PATH=./dist nodemon --verbose",
        "docs": "typedoc --tsconfig tsconfig.json",
        "lint:unusedExports": "ts-unused-exports tsconfig.eslint.json --ignoreTestFiles --showLineNumber --allowUnusedTypes --excludePathsFromReport=test",
        "lint:fix": "eslint --fix .",
        "lint": "eslint .",
        "lint:circular": "madge --circular dist/index.mjs",
        "mocha": "cross-env NODE_PATH=. mocha -r dotenv/config",
        "package:linux": "pkg --target node18-linux-x64 .",
        "package:windows": "pkg --target node18-win-x64 --options require=esm .",
        "package:windows:postfix": "ts-node --project tsconfig.eslint.json --esm scripts/postfixPackageWindows.mts",
        "prebuild": "concurrently \"npm run prebuild:rmDist\" \"npm run prebuild:nodeModuleFixes\"",
        "prebuild:rmDist": "rimraf dist",
        "prebuild:nodeModuleFixes": "ts-node --project tsconfig.eslint.json --esm scripts/nodeModuleFixes.mts",
        "start:inspect": "cross-env MOONPIE_CONFIG_LOGGING_CONSOLE_LOG_LEVEL=debug MOONPIE_CONFIG_TWITCH_DEBUG=ON node --inspect . -- --disable-censoring",
        "start:dev": "cross-env NODE_PATH=dist NODE_ENV=production node .",
        "start": "node .",
        "test": "cross-env NODE_PATH=. nyc mocha -- \"test/**/*.test.mts\"",
        "test:custom": "mocha -- "
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AnonymerNiklasistanonym/MoonpieBot.git"
    },
    "keywords": [],
    "author": "AnonymerNiklasistanonym",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/AnonymerNiklasistanonym/MoonpieBot/issues"
    },
    "homepage": "https://github.com/AnonymerNiklasistanonym/MoonpieBot",
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.4",
        "@types/irc": "^0.5.1",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.15.11",
        "@types/node-cron": "^3.0.7",
        "@types/sinon-chai": "^3.2.9",
        "@types/spotify-web-api-node": "^5.0.7",
        "@types/sqlite3": "^3.1.8",
        "@types/tmi.js": "^1.8.3",
        "@types/ws": "^8.5.4",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "chai": "^4.3.7",
        "concurrently": "^8.0.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.37.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-jsdoc": "^40.1.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-security": "^1.7.1",
        "eslint-plugin-typescript-sort-keys": "^2.3.0",
        "find-unused-exports": "^5.0.0",
        "madge": "^6.0.0",
        "mocha": "^10.2.0",
        "nodemon": "^2.0.22",
        "nyc": "^15.1.0",
        "pkg": "^5.8.1",
        "prettier": "^2.8.7",
        "resedit": "^2.0.0",
        "rimraf": "^4.4.1",
        "sinon": "^15.0.3",
        "sinon-chai": "^3.7.0",
        "ts-node": "^10.9.1",
        "ts-unused-exports": "^9.0.4",
        "typedoc": "^0.23.28",
        "typedoc-plugin-coverage": "^2.0.0",
        "typedoc-plugin-mermaid": "^1.10.0",
        "typedoc-umlclass": "^0.7.0",
        "typescript": "^5.0.3"
    },
    "dependencies": {
        "@twurple/api": "^6.1.0",
        "@twurple/auth": "^6.1.0",
        "commander": "^10.0.0",
        "csv-parser": "^3.0.0",
        "deep-copy-ts": "^0.5.4",
        "dotenv": "^16.0.3",
        "irc": "github:AnonymerNiklasistanonym/node-irc#91abd89",
        "node-cron": "^3.0.2",
        "node-fetch": "^3.3.1",
        "open": "^9.1.0",
        "osu-api-v2": "^0.0.33",
        "reconnecting-websocket": "^4.4.0",
        "semver": "^7.3.8",
        "spotify-web-api-node": "^5.0.2",
        "sqlite3-promise-query-api": "^0.0.10",
        "tmi.js": "^1.8.5",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1"
    }
}
