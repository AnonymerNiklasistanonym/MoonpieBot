{
    "name": "moonpiebot",
    "version": "1.0.12",
    "description": "A custom twitch bot",
    "main": "dist",
    "bin": "dist/index.js",
    "pkg": {
        "scripts": [
            "dist/**/*.js",
            "node_modules/es-get-iterator/node.js",
            "node_modules/sqlite3/lib/binding/**/*.node"
        ],
        "assets": [],
        "outputPath": "./bin"
    },
    "directories": {
        "test": "test"
    },
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "start": "node .",
        "start:dev": "cross-env NODE_PATH=./dist NODE_ENV=production node .",
        "prebuild": "rimraf ./dist/*",
        "build": "tsc",
        "package:win": "pkg --target node18-win-x64 .",
        "package:linux": "pkg --target node18-linux-x64 .",
        "test": "cross-env NODE_PATH=. nyc mocha -- \"test/**/*.test.ts\"",
        "docs": "typedoc --tsconfig tsconfig.json",
        "dev": "cross-env NODE_PATH=./dist nodemon --verbose",
        "build:dev": "tsc --sourceMap --removeComments false --pretty",
        "mocha": "cross-env NODE_PATH=. mocha -r dotenv/config",
        "lint": "eslint --ext .js,.ts .",
        "lint:fix": "eslint --fix --ext .js,.ts ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AnonymerNiklasistanonym/MoonpieBot.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/AnonymerNiklasistanonym/MoonpieBot/issues"
    },
    "homepage": "https://github.com/AnonymerNiklasistanonym/MoonpieBot#readme",
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.1",
        "@types/irc": "^0.5.1",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.6.1",
        "@types/node-cron": "^3.0.2",
        "@types/sinon-chai": "^3.2.8",
        "@types/spotify-web-api-node": "^5.0.7",
        "@types/sqlite3": "^3.1.8",
        "@types/tmi.js": "^1.8.1",
        "@types/ws": "^8.5.3",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "chai": "^4.3.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jsdoc": "^39.3.3",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-security": "^1.5.0",
        "mocha": "^10.0.0",
        "nodemon": "^2.0.19",
        "nyc": "^15.1.0",
        "pkg": "^5.8.0",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "sinon": "^14.0.0",
        "sinon-chai": "^3.7.0",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.9",
        "typedoc-plugin-coverage": "^2.0.0",
        "typedoc-plugin-mermaid": "^1.9.0",
        "typedoc-umlclass": "^0.7.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@twurple/api": "^5.1.8",
        "@twurple/auth": "^5.1.8",
        "csv-parser": "^3.0.0",
        "dotenv": "^16.0.1",
        "irc": "github:AnonymerNiklasistanonym/node-irc#91abd89",
        "node-cron": "^3.0.1",
        "open": "^8.4.0",
        "osu-api-v2": "^0.0.19",
        "reconnecting-websocket": "^4.4.0",
        "spotify-web-api-node": "^5.0.2",
        "sqlite3": "5.0.10",
        "tmi.js": "^1.8.5",
        "winston": "^3.8.1",
        "winston-daily-rotate-file": "^4.7.1"
    }
}
